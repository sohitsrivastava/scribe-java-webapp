pipeline {

 agents any
 PROJECT_ROOT=""
 stages{
 
	stage("SCM Checkout"){
		echo "Git Checkout Stage"
		git "https://github.com/sohitsrivastava/scribe-java-webapp.git"
	}
	stage("Build Stage"){
		echo "Building project"
		PROJECT_ROOT="${WORKSPACE}/scripe-java-webapp"
		dir("${PROJECT_ROOT}"){
			pwd
			ls -lrt
			sh "mvn clean package"
		}
	 
	}
 }
}
